<Window x:Class="BozosCircusTestHarness.SmartStart"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="SmartStart" Height="200" Width="350"
        Background="Transparent"
    ResizeMode="NoResize" WindowStyle="None" Foreground="{x:Null}" Opacity="1" AllowsTransparency="True">
	<Window.Resources>
		<LinearGradientBrush x:Key="SmartStart_Background" EndPoint="0.5,1" StartPoint="0.5,0">
			<GradientStop Color="#FF1C244D" Offset="1"/>
			<GradientStop Color="#FF495DBF" Offset="0"/>
		</LinearGradientBrush>
	</Window.Resources>
    <Grid>
    	<Grid.Background>
    		<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
    			<GradientStop Color="#FF000000" Offset="0"/>
    			<GradientStop Color="#FFFFFFFF" Offset="1"/>
    		</LinearGradientBrush>
    	</Grid.Background>
    	<Rectangle Stroke="#FF000000" RadiusX="10" RadiusY="10" Fill="{DynamicResource SmartStart_Background}" x:Name="CommandFinder"/>
        
    </Grid>
</Window>
